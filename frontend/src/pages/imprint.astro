---
import ArticleLayout from '~/layouts/ArticleLayout.astro';
import * as imprint from '~/../data/pages/imprint.md';
import { Meta } from '~/components/core/MetaTags.astro';

const meta: Meta = {
  title: imprint.frontmatter.title,
  description: imprint.frontmatter.desc,
};
---

<ArticleLayout meta={meta}>
  <imprint.Content />
</ArticleLayout>
